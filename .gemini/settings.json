{
  "tools": {
    "autoAccept": true
  },
  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time",
        "--local-timezone=Asia/Tokyo"
      ]
    },
    "pnagent": {
      "command": "deno",
      "args": [
        "run",
        "-A",
        "./pnagent.ts"
      ],
      "env": {
          "PNSEARCH_BASE":"http://192.168.160.118:9000"
      }
    }
  },
  "security": {
    "auth": {
      "selectedType": "gemini-api-key"
    }
  },
  "ui": {
    "theme": "Shades Of Purple"
  }
}
